package com.dihuo.twodfire.retail.data;

import java.sql.Date;

import android.R.integer;
/**
 * 用户信息
 * @author kyolee
 *
 */
public class UserVo {
	private String userId;//用户ID
	private String userName;//登陆名称
	private String name;//姓名
	private String staffId;//工号
	private Date inDate;//入职时间
	private String mobile;//手机号
	private integer sex;//性别
	private Date identityNo;//证件编号
	private integer identityTypeId;//证件类型
	private String address;//住址
	
	
	
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getUserName() {
		return userName;
	}
	public void setUserName(String userName) {
		this.userName = userName;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getStaffId() {
		return staffId;
	}
	public void setStaffId(String staffId) {
		this.staffId = staffId;
	}
	public Date getInDate() {
		return inDate;
	}
	public void setInDate(Date inDate) {
		this.inDate = inDate;
	}
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	public integer getSex() {
		return sex;
	}
	public void setSex(integer sex) {
		this.sex = sex;
	}
	public Date getIdentityNo() {
		return identityNo;
	}
	public void setIdentityNo(Date identityNo) {
		this.identityNo = identityNo;
	}
	public integer getIdentityTypeId() {
		return identityTypeId;
	}
	public void setIdentityTypeId(integer identityTypeId) {
		this.identityTypeId = identityTypeId;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}

}
